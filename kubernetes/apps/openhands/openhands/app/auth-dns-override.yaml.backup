---
apiVersion: externaldns.k8s.io/v1alpha1
kind: DNSEndpoint
metadata:
  name: auth-openhands-override
  namespace: openhands
  annotations:
    external-dns.alpha.kubernetes.io/cloudflare-proxied: "false"
spec:
  endpoints:
    - dnsName: "auth.openhands.${SECRET_DOMAIN}"
      recordType: A
      targets:
        - "192.168.0.184"
      setIdentifier: "auth-override"